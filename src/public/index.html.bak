<html lang="en">
   <head>
      <meta name="description" content="David James Portfolio">
      <meta name="author" content="David James">
      <meta name="viewport" content="width=device-width, initial-scale=1">
      <link rel="stylesheet" href="css/index.css">
      <link rel=stylesheet href=https://cdn.rawgit.com/openlayers/openlayers.github.io/master/en/v5.1.3/css/ol.css type=text/css>
      <script src=https://cdn.rawgit.com/openlayers/openlayers.github.io/master/en/v5.1.3/build/ol.js></script>
   </head>
   <div id="viewHead"></div>
   <body onload="changePage('about');">
      <script src="js/index.js"></script>
      <div id="siteContainer">
         <div id="title">
            <h1 id="titleLeft">David James</h1>
            <h2 id="titleRight"> Portfolio </h2>
            <div id="hamburger" class="buttonStyles" onclick="toggleMobileNav()">
               <div class="mobileNavButton"></div>
               <div class="mobileNavButton"></div>
               <div class="mobileNavButton"></div>
            </div>

         </div>

         <div id="nav" class="mediahide">
            <div tplName="about" onclick="changePage(this.getAttribute('tplName'))" class="buttonStyles navItem">About</div>
            <div tplName="projects" onclick="changePage(this.getAttribute('tplName'))" class="buttonStyles navItem">Projects</div>
            <div tplName="adventures" onclick="changePage(this.getAttribute('tplName'))" class="buttonStyles navItem">Adventures</div>
         </div>
         <div id="main">
         </div>
      </div>
   </body>
</html>
