#circle{
   margin:10px;
   width:800px;
   height:800px;
}
@media only screen and (max-width: 1000px){
   #circle {
      display:none;
   }
   #mobileRows{
      display:block !important;
   }
}
#mobileRows {
   display:none;
}
.infoRow {
   width:100%;
   height:25%;
   border: 1px solid black;
}
.circleHalf {
   width:100%;
   height:50%;
   display:flex;
   justify-content:center;
}
.circleUL {
   width:50%;
   height:100%;
   background-color:var(--sub-color-fade);
   border-radius : 100% 0 0 0;
}

.circleUR {
   width:50%;
   height:100%;
   background-color:var(--title-color-fade);
   border-radius : 0 100% 0 0;
}
.circleLL {
   width:50%;
   height:100%;
   background-color:var(--title-color-fade);
   border-radius : 0 0 0 100%;
}
.circleLR {
   width:50%;
   height:100%;
   background-color: var(--sub-color-fade);
   border-radius : 0 0 100% 0;
}
.hideCircle {
   background-color:white;
}
.hoverHide:hover {
   background-color:var(--sub-color-fade);
   transition: background-color .25s linear;
}
.circleTextWrapper {
   position:relative;
   height:100%;
   width:100%;
}
.top {
   bottom:0;
}
.right {
   left:0;
}
.textLeft {
   width:75%;
   float:right;
   padding:5px;
}
.textRight {
   width:75%;
   float:left;
   padding:5px;
}
.bottom {
   top:0;
}
.left {
   right:0;
}
.circleText {
   position:absolute;
   font-size:30px;
}
#circleContainer {
   display:flex;
   justify-content:center;
}
.circleTitle {
   position:absolute;
   font-size:45px;
}
.circleTitlePosUR {
   top:50%;
   left:25%;
   
}
.circleTitlePosUL {
   top:50%;
   right:25%;
}
.circleTitlePosLR {
   bottom:50%;
   left:25%;
   
}
.circleTitlePosLL {
   bottom:50%;
   right:25%;
}

